{
    "name": "chack1172/laravel-installer",
    "description": "The Laravel application installer with modern dev tools.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "chack1172",
            "email": "chack1172@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^10.20|^11.0|^12.0",
        "illuminate/support": "^10.20|^11.0|^12.0",
        "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/process": "^6.2|^7.0",
        "symfony/polyfill-mbstring": "^1.31"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.4"
    },
    "bin": [
        "bin/laravel"
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
